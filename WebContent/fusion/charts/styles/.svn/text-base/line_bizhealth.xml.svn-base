<?xml version="1.0" encoding="UTF-8"?>
<chart bgAlpha="0" bgColor="FFFFFF" showYAxisValues="0"
	showValues="0" showBorder="0" canvasBorderThickness="1" showLabels="0"
	canvasBorderColor="666666" alternateHGridColor="FCB541"
	alternateHGridAlpha="20" divLineColor="FCB541" divLineAlpha="50"
	baseFontColor="666666" lineColor="FCB541" chartTopMargin="5"
	adjustDiv="0" yAxisMinValue="0" yAxisMaxValue="4" numDivLines="3" decimals="1">
	%DATA%
	
	<trendLines>
		<line startValue="0" endValue="1" isTrendZone="1" color="afd8f8"
			displayvalue="正常" alpha="0"/>
		<line startValue="1" endValue="2" isTrendZone="1" color="cdc309"
			displayvalue="低风险" alpha="0"/>
		<line startValue="2" endValue="3" isTrendZone="1" color="f6bd0f"
			displayvalue="中风险" alpha="0"/>
		<line startValue="3" endValue="4" isTrendZone="1" color="ff654f"
			displayvalue="高风险" alpha="0"/>
	</trendLines>
	
	<styles>
		<definition>
			<style name="Anim1" type="animation" param="_xscale" start="0"
				duration="1"/>
			<style name="Anim2" type="animation" param="_alpha" start="0"
				duration="0.6"/>
			<style name="DataShadow" type="Shadow" alpha="40"/>
			<style name="labelFont" type="font" size="12" bold="0"/>
		</definition>
		<application>
			<apply toObject="DIVLINES" styles="Anim1"/>
			<apply toObject="HGRID" styles="Anim2"/>
			<apply toObject="DATALABELS" styles="DataShadow,Anim2"/>
			<apply toObject="TRENDVALUES" styles="labelFont"/>
		</application>
	</styles>
	
</chart>