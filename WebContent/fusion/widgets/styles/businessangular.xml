<?xml version="1.0" encoding="UTF-8"?>
<chart bgAlpha="0" bgColor="FFFFFF" lowerLimit="0" upperLimit="4" showBorder="0"
	basefontColor="000000" gaugeOuterRadius="160" gaugeInnerRadius="100"
	showGaugeBorder="1" gaugeBorderColor="666666" gaugeBorderThickness="1"
	showTickValues="0" showTickMarks="0" autoScale="1" origW="350" origH="200"
	pivotRadius="15" pivotBgColor="333333" pivotBorderColor="FFFFFF"
	pivotFillMix="" pivotBorderThickness="2" showPivotBorder="1">
	<colorRange>
		<color minValue="0" maxValue="1" code="afd8f8"/>
		<color minValue="1" maxValue="2" code="cdc309"/>
		<color minValue="2" maxValue="3" code="f6bd0f"/>
		<color minValue="3" maxValue="4" code="ff654f"/>
	</colorRange>
	
	<dials>
		<dial value="%DATA%" bgColor="333333" radius="95" borderColor="FFFFFF"
			borderAlpha="100" baseWidth="30" topWidth="1" borderThickness="2"/>
	</dials>
	
	<trendpoints>
		<point displayValue="正常" startValue="0" endValue="1" radius="100"
			innerRadius="40" color="afd8f8" alpha="30" borderColor="FFFFFF"/>
		<point displayValue="低风险" startValue="1" endValue="2" radius="100"
			innerRadius="40" color="cdc309" alpha="30" borderColor="FFFFFF"/>
		<point displayValue="中风险" startValue="2" endValue="3" radius="100"
			innerRadius="40" color="f6bd0f" alpha="30" borderColor="FFFFFF"/>
		<point displayValue="高风险" startValue="3" endValue="4" radius="100"
			innerRadius="40" color="ff654f" alpha="30" borderColor="FFFFFF"/>
	</trendpoints>
	
	<styles>
		<definition>
			<style name="labelFont" type="font" size="12" color="FFFFFF" bold="1"
				isHTML="1"/>
			<style name="labelGlow" type="glow" color="000000" alpha="45"/>
		</definition>
		<application>
			<apply toObject="TRENDVALUES" styles="labelFont,labelGlow"/>
		</application>
	</styles>
</chart>