<chart bgAlpha='0' bgColor='FFFFFF' lowerLimit='0' upperLimit='100'
numberSuffix='%25' showBorder='0' basefontColor='666666' 
chartTopMargin='10' chartBottomMargin='-20' toolTipBgColor='80A905' 
gaugeStartAngle='225' gaugeEndAngle='-45' gaugeFillMix="{dark-10},{light-60}" gaugeFillRatio='28' 
showTickMarks="1" showTickValues="1" majorTMNumber='6' placeTicksInside='1' majorTMHeight='10' minorTMHeight='5' 
placeValuesInside='1' showValue="1" valueBelowPivot="1"
pivotRadius="8" pivotFillColor="000000,383836" pivotFillType="linear" pivotFillRatio="50,50" pivotFillAngle="240"
autoScale="1" origW="240" origH="180"> 

   <colorRange>
      <color minValue='0' maxValue='50' code='8BBA00'/>
      <color minValue='50' maxValue='80' code='F6BD0F'/>
      <color minValue='80' maxValue='100' code='FF654F'/>
   </colorRange>

   <dials>
      %DATA%
   </dials>

	<styles>
      <definition>
	    <style type="font" name="valueFont" size="11" color="768688" bold="1" />
		<style type="font" name="tickFont" size="8" />
      </definition>
      <application>
	    <apply toObject="Value" styles="valueFont" />
		<apply toObject="TICKVALUES" styles="tickFont" />
		<apply toObject="LIMITVALUES" styles="tickFont" />
      </application>
   </styles>
</chart>